<+>h1,\r\nh2,\r\nh3,\r\nh4,\r\nh5,\r\nh6,\r\np {\r\n  margin: 0;\r\n  padding: 0;\r\n}\r\n.overflow-hidden {\r\n  overflow: hidden;\r\n}\r\n\r\n.modal {\r\n  display: block;\r\n  z-index: 20000;\r\n  position: absolute;\r\n  top: 0;\r\n  right: 0;\r\n  height: 100vh;\r\n  width: 552px;\r\n  transition: 0.5s;\r\n  transform: translateX(100%);\r\n  background-color: $graphicWhite100;\r\n  &__backdrop {\r\n    display: none;\r\n    width: 100vw;\r\n    height: 100vh;\r\n    position: fixed;\r\n    top: 0;\r\n    z-index: 90;\r\n    background: rgba(178, 170, 170, 0.25);\r\n    backdrop-filter: blur(4px);\r\n    transition: 0.2s;\r\n  }\r\n  &__wrapper {\r\n    padding: 40px 24px 0;\r\n    width: 100%;\r\n    height: 86%;\r\n    //position: relative;\r\n    .category-flex {\r\n      flex-wrap: wrap;\r\n      .category__product {\r\n        width: 48%;\r\n      }\r\n    }\r\n  }\r\n  &__heading {\r\n    font-size: 18px;\r\n    font-weight: 500;\r\n    line-height: 25px;\r\n    text-align: center;\r\n    text-transform: uppercase;\r\n    padding: 38px 0 12px;\r\n    max-width: 365px;\r\n    margin: 0 auto;\r\n  }\r\n  &__scrolling-block {\r\n    overflow-y: scroll;\r\n    height: 100%;\r\n  }\r\n  &__title {\r\n    display: flex;\r\n    align-items: center;\r\n    padding: 0 24px;\r\n    border-bottom: $graphicBlack10 solid 0.5px;\r\n    position: relative;\r\n    height: 64px;\r\n    text-transform: uppercase;\r\n    font-size: 18px;\r\n    font-weight: 500;\r\n    line-height: 25px;\r\n    .close {\r\n      display: flex;\r\n      position: absolute;\r\n      right: 24px;\r\n      top: 16px;\r\n      width: 32px;\r\n      height: 32px;\r\n      padding: 6px;\r\n      span {\r\n        display: block;\r\n        position: absolute;\r\n        right: 2px;\r\n        top: 15px;\r\n        width: 27px;\r\n        height: 1px;\r\n        background: $graphicBlack;\r\n      }\r\n      span:nth-child(1) {\r\n        transform: rotate(45deg);\r\n      }\r\n      span:nth-child(2) {\r\n        transform: rotate(-45deg);\r\n      }\r\n    }\r\n  }\r\n  &__question,\r\n  &__answer {\r\n    text-align: left;\r\n  }\r\n  &__question {\r\n    font-weight: bold;\r\n    margin-bottom: 12px;\r\n  }\r\n  &__answer {\r\n    margin-bottom: 40px;\r\n  }\r\n  &__text {\r\n    max-width: 400px;\r\n    text-align: left;\r\n  }\r\n  &__item {\r\n    //height: 65px;\r\n    padding: 20px 24px;\r\n    border-bottom: $graphicBlack10 solid 0.5px;\r\n    font-size: 18px;\r\n    font-weight: 500;\r\n    line-height: 25px;\r\n    text-align: left;\r\n    text-transform: uppercase;\r\n    position: relative;\r\n    .modal__cross {\r\n      position: absolute;\r\n      right: 24px;\r\n      bottom: 20px;\r\n      width: 24px;\r\n      height: 24px;\r\n      padding: 8px;\r\n      span {\r\n        display: block;\r\n        position: absolute;\r\n        bottom: 12px;\r\n        width: 8px;\r\n        height: 1px;\r\n        background: $graphicBlack50;\r\n      }\r\n      span:nth-child(1) {\r\n        transform: rotate(90deg);\r\n        transform-origin: center;\r\n        transition: 0.3s;\r\n      }\r\n    }\r\n  }\r\n  &__view-height {\r\n    height: 84vh;\r\n    display: flex;\r\n    flex-direction: column;\r\n    padding: 0 24px 40px;\r\n  }\r\n  &__content {\r\n    display: none;\r\n    //display: flex;\r\n    flex-wrap: wrap;\r\n    padding: 0 24px 48px 24px;\r\n    text-align: left;\r\n    gap: 8px;\r\n    color: $textBlack60;\r\n    &--block {\r\n      display: none;\r\n      //display: block;\r\n    }\r\n  }\r\n  &__list {\r\n    padding-left: 16px;\r\n    &-item {\r\n      margin-bottom: 8px;\r\n      color: $textBlack10;\r\n    }\r\n  }\r\n  &__option {\r\n    width: 48%;\r\n    cursor: pointer;\r\n    &--active {\r\n      color: $textgold;\r\n    }\r\n  }\r\n\r\n  &__footer {\r\n    .modal__wrapper {\r\n      padding-top: 0;\r\n    }\r\n  }\r\n  &__button {\r\n    width: 100%;\r\n    padding: 12px 24px;\r\n    text-transform: uppercase;\r\n    transition: 0.5s;\r\n    border: none;\r\n    font-size: 14px;\r\n    font-weight: 500;\r\n    line-height: 14px;\r\n    letter-spacing: -0.01em;\r\n    text-align: center;\r\n    &--black {\r\n      background: $graphicBlack;\r\n      color: $textWhite100;\r\n      bottom: 81px;\r\n    }\r\n    &--white {\r\n      background: $graphicWhite100;\r\n      color: $textBlack10;\r\n      bottom: 24px;\r\n      border: $graphicBlack 1px solid;\r\n      &:hover {\r\n        border: $graphicgold 1px solid;\r\n        color: $textgold;\r\n      }\r\n      &:active {\r\n        border: $graphicgold 1px solid;\r\n        background: $graphicgold;\r\n        color: $textWhite100;\r\n      }\r\n      &:disabled {\r\n        border: $graphicBlack50 1px solid;\r\n        color: $textBlack60;\r\n      }\r\n    }\r\n  }\r\n  &__button--black:hover {\r\n    background: $graphicBlack50;\r\n  }\r\n  &__button--black:active {\r\n    background: $graphicBlack30;\r\n  }\r\n  &__button--black:disabled {\r\n    background: $graphicBlack30;\r\n    color: $textWhite50;\r\n  }\r\n}\r\n.flex-wrapper {\r\n  //max-width: 369px;\r\n  margin: 0 auto;\r\n  padding-top: 80px;\r\n\r\n}\r\n//кнопка сбросить на модалке-фильтре\r\n#reset {\r\n  display: none;\r\n  border: none;\r\n}\r\n.border-none {\r\n  border-bottom: none;\r\n}\r\n.filter-active {\r\n  display: block;\r\n}\r\n.position-bottom {\r\n  position: absolute;\r\n  bottom: 40px;\r\n  width: 90%;\r\n}\r\n.modal-active {\r\n  transform: translateX(0);\r\n}\r\n#ship-and-pay,\r\n#info-return {\r\n  //display: none;\r\n}\r\n#wishlist {\r\n  overflow: scroll;\r\n  .modal__button {\r\n    width: 356px;\r\n  }\r\n}\r\n\r\n@media screen and (max-width: 800px) {\r\n  .modal {\r\n    width: 100vw;\r\n    &__button--white {\r\n      width: 100%;\r\n    }\r\n    &__heading {\r\n      max-width: 420px;\r\n    }\r\n    &__view-height {\r\n      justify-content: space-between;\r\n      padding-top: 140px;\r\n    }\r\n  }\r\n  .flex-wrapper {\r\n    margin: 0 auto;\r\n    padding-top: 0;\r\n    width: 100%;\r\n  }\r\n  #wishlist {\r\n    .modal__button {\r\n      width: 100%;\r\n    }\r\n  }\r\n}\r\n@media screen and (max-width: 528px) {\r\n  .modal {\r\n    width: 100vw;\r\n    &__title {\r\n      font-size: 16px;\r\n      font-weight: 500;\r\n      line-height: 21px;\r\n      text-align: left;\r\n    }\r\n    &__heading {\r\n      font-size: 16px;\r\n      font-weight: 500;\r\n      line-height: 21px;\r\n      text-align: center;\r\n    }\r\n  }\r\n}\r\n\r\n
